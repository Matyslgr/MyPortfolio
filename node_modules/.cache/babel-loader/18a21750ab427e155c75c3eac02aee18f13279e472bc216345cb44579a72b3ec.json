{"ast":null,"code":"import HomeView from './components/Home.vue';\nimport ProjectList from './components/Project.vue';\nimport NavBar from './components/Navbar.vue';\nimport About from './components/About.vue';\nimport Skills from './components/Skills.vue';\nimport FooterView from './components/Footer.vue';\nexport default {\n  name: 'App',\n  components: {\n    HomeView,\n    ProjectList,\n    NavBar,\n    About,\n    Skills,\n    FooterView\n  }\n};","map":{"version":3,"names":["HomeView","ProjectList","NavBar","About","Skills","FooterView","name","components"],"sources":["src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\" class=\"full-page\">\n    <NavBar/>\n    <div id=\"home\">\n      <HomeView/>\n    </div>\n    <div id=\"about\" class=\"section\">\n      <About/>\n    </div>\n    <div id=\"skills\" class=\"section\">\n      <Skills/>\n    </div>\n    <div id=\"projects\" class=\"section\">\n      <ProjectList/>\n    </div>\n    <div id=\"contacts\" class=\"section\">\n      <FooterView/>\n    </div>\n    <br>\n    <br>\n    <br>\n    <br>\n    <br>\n    <br>\n    <div class=\"bubbles-container\">\n      <div class=\"bubble\">\n        <span></span>\n        <span></span>\n        \n      </div>\n      <div class=\"bubble\"></div>\n      <div class=\"bubble\"></div>\n      <div class=\"bubble\"></div>\n      <div class=\"bubble\"></div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport HomeView from './components/Home.vue'\nimport ProjectList from './components/Project.vue';\nimport NavBar from './components/Navbar.vue';\nimport About from './components/About.vue';\nimport Skills from './components/Skills.vue';\nimport FooterView from './components/Footer.vue';\n\nexport default {\n  name: 'App',\n  components: {\n    HomeView,\n    ProjectList,\n    NavBar,\n    About,\n    Skills,\n    FooterView\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  color: #2c3e50;\n  margin-top: 4%;\n}\n\nbody {\n  margin: 0;\n  padding: 0;\n  overflow-x: hidden;\n  background: url('assets/fond4.jpg') center/cover no-repeat;\n}\n\n.section {\n  margin: 0;\n}\n\n#app.full-page {\n  min-height: 100vh;\n}\n.bubbles-container {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  pointer-events: none; /* Assure que les bulles n'interfèrent pas avec les éléments de la page */\n  z-index: -1; /* Les bulles sont en arrière-plan */\n}\n\n.bubble {\n  position: absolute;\n  width: 200px;\n  height: 200px;\n  border-radius: 50%;\n  box-shadow: inset 0 0 25px rgba(255, 255, 255, 0.25);\n  animation: animate_4010 8s ease-in-out infinite;\n}\n.bubble:nth-child(2) {\n  position: absolute;\n  width: 150px;\n  height: 150px;\n  left: 10%;\n  top: 20%;\n  animation-delay: -4s;\n}\n\n.bubble:nth-child(3) {\n  position: absolute;\n  width: 150px;\n  height: 150px;\n  right: 8%;\n  top: 40%;\n  animation-delay: -6s;\n}\n\n.bubble:nth-child(4) {\n  position: absolute;\n  width: 170px;\n  height: 170px;\n  left: 5%;\n  bottom: 30%;\n  animation-delay: -3s;\n}\n\n.bubble:nth-child(5) {\n  position: absolute;\n  width: 180px;\n  height: 180px;\n  left: 50%;\n  top: 70%;\n  animation-delay: -5s;\n}\n\n@keyframes animate_4010 {\n  0%, 100% {\n    transform: translateY(-20px);\n  }\n\n  50% {\n    transform: translateY(20px);\n  }\n}\n</style>\n"],"mappings":"AAuCA,OAAAA,QAAA;AACA,OAAAC,WAAA;AACA,OAAAC,MAAA;AACA,OAAAC,KAAA;AACA,OAAAC,MAAA;AACA,OAAAC,UAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAP,QAAA;IACAC,WAAA;IACAC,MAAA;IACAC,KAAA;IACAC,MAAA;IACAC;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}