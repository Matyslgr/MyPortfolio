ound-clip: text; /* Pour appliquer le dégradé au texte seulement */\n  font-size: 2em;\n  font-family: \"Home\", sans-serif;\n  font-style: normal;\n  position: relative;\n  top: -70vh;\n  left: 130vh;\n}\n.social-bar {\n  background-color: rgba(255, 255, 255, 0.5);\n  border-radius: 30px;\n  position: relative;\n  max-width: 5%;\n  top: -650px;\n  left: 93%;\n  transform: translate(0, -50%);\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.social-bar li {\n  margin: 0;\n  padding-top: 10px;\n  padding-bottom: 10px;\n}\n\n.social-bar ul {\n  list-style: none;\n  padding: 10px;\n  margin: 0;\n  display: flex;\n  flex-direction: column;\n  align-self: stretch;\n  justify-content: space-around;\n}\n\n.social-img {\n  height: auto;\n  width: 50px;\n  transition: transform 0.4s, filter 0.4s;\n}\n\n.social-bar li:not(:hover) .social-img {\n  filter: blur(3px);\n}\n\n.social-bar img:hover {\n  transform: scale(1.1, 1.1);\n}\n\n/* Colors for social icons */\n.social-icon.red {\n  filter: brightness(180%);\n}\n\n.social-icon.blue {\n  filter: brightness(110%);\n}\n\n.social-icon.green {\n  filter: brightness(100%);\n}\n\n.social-icon.purple {\n  filter: brightness(90%);\n}\n</style>\n"],"mappings":"AAoBA;EACAA,IAAA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}